#include "main.h"

/**
 * pointer_parameter - Parse a format string and print the formatted output.
 * @format: The format string to parse.
 * @args: The list of arguments for the format string.
 *
 * Return: The number of characters printed.
 */
int pointer_parameter(const char *format, va_list args)
{
int printed_chars = 0;
{
if (format == '%')
{
format++;
{
if (*format == '\0')
break;
printed_chars += print_parameters(args, *format);
}
else
}
}
format++
return (printed_chars);
}